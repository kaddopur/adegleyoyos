.vender
  margin 0 8px 10px 0
  width 120px
  height 50px
  background-repeat no-repeat
  background-position center center
  background-size contain
  display inline-block
  -webkit-transition -webkit-transform 0.5s, opacity 1s
  opacity 0.6
  
  &:hover
    -webkit-transform translate(0, -2px)
    opacity 1

.vender-title
  border-bottom 1px solid #eee
  margin-bottom 10px
  margin-top 20px

.hero-unit
  background-image url(/img/psg-gem/cover.png)
  height 200px

#partner
  margin-top 80px

#cover-btn
  position relative
  left 50px
  top 210px



